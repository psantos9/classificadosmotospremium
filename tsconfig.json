{
  "compilerOptions": {
    "incremental": false,
    "composite": false,
    "target": "ES6",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "types": ["@cloudflare/workers-types"],
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
